<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onroadfuel">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/l1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnRoadFuel"
        android:usesCleartextTraffic="true">
        <activity android:name=".User_send_complaints"></activity>
        <activity android:name=".Driver_view_ratings" />
        <activity android:name=".User_review_rating" />
        <activity android:name=".Driver_update_stock" />
        <activity android:name=".User_make_payment" />
        <activity android:name=".Driver_view_booking" />
        <activity android:name=".User_view_request" />
        <activity android:name=".User_send_fuel_request" />
        <activity android:name=".User_view_near_by_vehicle" />
        <activity android:name=".Driver_view_assinged_vehicle" />
        <activity android:name=".User_registration" />
        <activity android:name=".Driver_home" />
        <activity android:name=".Login" />
        <activity android:name=".Users_home" />
        <activity android:name=".IPSetting">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <service android:name=".LocationService" />
    </application>

</manifest>